{# Layout de base du module RH #}
{% extends "base.html.twig" %}

{% block title %}
    RH {{ parent() }}
{% endblock %}

{% block content %}

    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">
                {% block content_title %}
                    Gestion RH
                {% endblock %}
            </h3>
            {% block breadcrumb %}
                <ul class="breadcrumb no-margin pull-right">
                    <li>
                        {{ 'excel.importation'|trans({}, 'excel') }}
                    </li>
                </ul>
            {% endblock %}
        </div>
        <div class="box-body">
            {# On d√©finit un nouveau block, que les vues du bundle pourront remplir #}
            {% block content_bundle %}
            {% endblock %}

        </div>
        <div class="box-footer"></div>
    </div>

{% endblock %}
